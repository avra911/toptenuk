<layout version="0.1.0">
    <default>
	<reference name="head">
	    <action method="addJs" ifconfig="jquerylib/general/next"><script>jquery/jquery-1.8.3.min.js</script></action>
	    <action method="addJs" ifconfig="jquerylib/general/enabled"><script>jquery/jquery-1.6.4.min.js</script></action>
	    
	    <action method="addJs" ifconfig="jquerylib/general/enabled"><script>jquery/mygento/jquery.init.js</script></action>
	    <action method="addJs" ifconfig="jquerylib/general/next"><script>jquery/mygento/jquery.init.js</script></action>

	    <action method="addJs" ifconfig="jquerylib/plugins/easing"><script>jquery/jquery.easing.1.3.js</script></action>
	    <action method="addJs" ifconfig="jquerylib/plugins/carousel"><script>jquery/jquery.jcarousel.min.js</script></action>

	    <action method="addJs" ifconfig="jquerylib/plugins/ui"><script>jquery/jqueryui/jquery-ui.min.js</script></action>
	    <action method="addItem" ifconfig="jquerylib/plugins/ui">
			<type>js_css</type>
			<name>jquery/jqueryui/jquery-ui.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/mobile"><script>jquery/jquery.mobile-1.2.0.min.js</script></action>

	    <action method="addJs" ifconfig="jquerylib/plugins/hovercard"><script>jquery/jquery.hovercard.min.js</script></action>
	    <action method="addJs" ifconfig="jquerylib/plugins/ddslick"><script>jquery/jquery.ddslick.min.js</script></action>
	    <action method="addJs" ifconfig="jquerylib/plugins/masked"><script>jquery/jquery.maskedinput.min.js</script></action>
		<action method="addJs" ifconfig="jquerylib/plugins/backstretch"><script>jquery/jquery.backstretch.min.js</script></action>
		<action method="addJs" ifconfig="jquerylib/plugins/modernizr"><script>jquery/jquery.modernizr.min.js</script></action>
		
	    <action method="addJs" ifconfig="jquerylib/plugins/chosen"><script>jquery/chosen/chosen.jquery.min.js</script></action>
	    <action method="addItem" ifconfig="jquerylib/plugins/chosen">
			<type>js_css</type>
			<name>jquery/chosen/chosen.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/galleria">
			<script>jquery/galleria/galleria-1.2.8.min.js</script>
	    </action>
	    <action method="addJs" ifconfig="jquerylib/plugins/galleria">
		<script>jquery/galleria/galleria.loadtheme.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/galleria">
			<type>js_css</type>
			<name>jquery/galleria/themes/classic/galleria.classic.css</name>
	    </action>


	    <action method="addJs" ifconfig="jquerylib/plugins/nivo">
		<script>jquery/nivo/nivo.slider.pack.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/nivo">
		<type>js_css</type>
		<name>jquery/nivo/nivo-slider.css</name>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/nivo">
		<type>js_css</type>
		<name>jquery/nivo/themes/default/default.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/slides">
		<script>jquery/slides/jquery.slides.min.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/slides">
		<type>js_css</type>
		<name>jquery/slides/slides.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/fancy">
		<script>jquery/fancybox/jquery.fancybox.pack.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/fancy">
		<type>js_css</type>
		<name>jquery/fancybox/jquery.fancybox.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/loupe">
		<script>jquery/loupe/jquery.loupe.min.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/loupe">
		<type>js_css</type>
		<name>jquery/loupe/loupe.css</name>
	    </action>


	    <action method="addJs" ifconfig="jquerylib/plugins/skitter">
		<script>jquery/skitter/jquery.skitter.min.js</script>
	    </action>
	    <action method="addJs" ifconfig="jquerylib/plugins/skitter">
		<script>jquery/skitter/jquery.animate-colors-min.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/skitter">
		<type>js_css</type>
		<name>jquery/skitter/css/skitter.styles.css</name>
	    </action>

	    <action method="addJs" ifconfig="jquerylib/plugins/tiptip">
		<script>jquery/tiptip/jquery.tipTip.min.js</script>
	    </action>
	    <action method="addItem" ifconfig="jquerylib/plugins/tiptip">
		<type>js_css</type>
		<name>jquery/tiptip/tipTip.css</name>
	    </action>
	</reference>
    </default>
</layout>